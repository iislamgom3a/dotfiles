{
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "vim_mode": true,
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Material Icon Theme",
  },
  "auto_install_extensions": {
    "sql": true,
    "dockerfile": true,
    "docker-compose": true,
    "github-theme": true,
    "material-icon-theme": true,
    "toml": true,
    "git-firefly": true,
    "lua": true,
    "tmux": true,
    "log": true,
  },
  "ui_font_size": 17,
  "buffer_font_size": 17,
  "agent_buffer_font_size": 16,
  "agent_ui_font_size": 17,

  "theme": {
    "mode": "dark",
    "light": "Github Light",
    "dark": "Github Dark",
  },
  "show_completion_documentation": true,
  "diagnostics": {
    "inline": {
      "enabled": true,
    },
  },
  "disable_ai": true,
  "git": {
    "inline_blame": {
      "enabled": false,
    },
  },
}
